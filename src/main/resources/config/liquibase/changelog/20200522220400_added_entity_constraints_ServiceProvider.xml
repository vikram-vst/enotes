<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity ServiceProvider.
    -->
    <changeSet id="20200522220400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="service_provider"
                                 constraintName="fk_service_provider_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sys_user"/>

        <addForeignKeyConstraint baseColumnNames="service_id"
                                 baseTableName="service_provider"
                                 constraintName="fk_service_provider_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service"/>

    </changeSet>
</databaseChangeLog>
